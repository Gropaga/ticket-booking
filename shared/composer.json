{
    "name": "spiral/shared",
    "description": "This is shared package",
    "keywords": [
        "spiral",
        "spiral",
        "open-telemetry-shared"
    ],
    "homepage": "https://github.com/spiral/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "butschster",
            "email": "butschster@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-grpc": "*",
        "grpc/grpc": "^1.42",
        "spiral/boot": "^3.7",
        "spiral/hmvc": "^3.7",
        "spiral/files": "^3.7",
        "spiral/console": "^3.7",
        "spiral/auth": "^3.7",
        "spiral/telemetry": "^3.7",
        "spiral-packages/cqrs": "^2.0",
        "spiral/roadrunner-grpc": "^3.0"
    },
    "require-dev": {
        "spiral/framework": "^3.7",
        "nette/php-generator": "^4.0",
        "spiral/testing": "^2.0",
        "vimeo/psalm": "^4.27"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Shared\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "psalm": "vendor/bin/psalm --config=psalm.xml ./src"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
